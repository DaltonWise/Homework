        "\n",
        "#we create a new filter where the proprietary name is null.\n",
        "\n",
        "blank_filter = ndc['PROPRIETARYNAME'].isnull()\n",
        "\n",
        "#We create a new data set with just the rows with the blanks.\n",
        "blanks = ndc[blank_filter]\n",

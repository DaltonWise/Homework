      "source": [
        "#We can use more complicated search criteria.\n",
        "#This first lineof code is creating a new filter where the name in the column 'hospital_name' ends in the word 'CENTER'.\\\n",
        "#\n",
        "\n",
        "med_center_filter = data['hospital_name'].str.contains('CENTER$')\n",
        "\n",
        "#Here we are creating a data set with only rows where teh filter applies.\n",
        "med_centers = data[med_center_filter]"
      ]
